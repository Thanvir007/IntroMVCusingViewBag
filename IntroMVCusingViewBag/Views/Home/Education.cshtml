
@{
    ViewBag.Title = "Education";
}

<h3>@ViewBag.Message</h3>

<div class="wrapper">
    <div class="container">
        <table class="table">
            <thead>
                <tr>
                    <th scope="col">Degree</th>
                    <th scope="col">Year</th>
                    <th scope="col">Institution</th>
                </tr>
            </thead>
            @foreach (var item in ViewBag.model)
            {
                <tbody>
                    <tr>
                        <th scope="row">@item.Degree</th>
                        <td>@item.Year</td>
                        <td>@item.Institution</td>
                    </tr>
                </tbody>
            }
        </table>
    </div>
</div>